{
  "presets": [
    ["es2015"],
    "react",
    "stage-0"
  ],
  "plugins": [
    ["import", {
        "libraryName": "antd",
        "style": "css"
    }],
    ["transform-runtime"],
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  }
}